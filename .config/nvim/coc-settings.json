{
  "suggest.detailField": "preview",
  "diagnostic.errorSign": "×",
  "diagnostic.infoSign": "!!",
  "diagnostic.hintSign": "??",
  "diagnostic.messageTarget": "echo",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": " ■ ",
  "signature.hideOnTextChange": true,
  "coc.preferences.bracketEnterImprove": false,
  "coc.preferences.extensionUpdateCheck": "never",
  "coc.preferences.formatOnSaveFiletypes": [ "go", "sh", "yaml"],
  "list.insertMappings": {
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-t>": "action:tabe",
    "<C-w>": "command:wincmd k"
  },
  "list.source.files.command": "rg",
  "list.source.files.args": ["--hidden", "--files"],
  "list.source.grep.args": [
    "--hidden"
  ],
  "git.enableGutters": true,
  "git.addedSign.hlGroup": "SignifySignAdd",
  "git.changedSign.hlGroup": "SignifySignChange",
  "git.removedSign.hlGroup": "SignifySignDelete",
  "git.topRemovedSign.hlGroup": "SignifySignDelete",
  "git.changeRemovedSign.hlGroup": "SignifySignDeleteFirstLine",
  "snippets.ultisnips.enable": false,
  "snippets.snipmate.enable": false,
  "translator.toLang": "ja",
  "translator.engines": ["google"],
  "translator.maxsize": 1000,
  "go.goplsOptions": {
    "hoverKind": "FullDocumentation",
    "completeUnimported": true,
    "usePlaceholders": true,
    "staticcheck": true,
    "analyses": {
      "fillreturns": true
    }
  },
  "markdownlint.config": {
    "MD013": false,
    "MD033": false,
    "MD024": {
      "siblings_only":true
    }
  },
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "sh": "shfmt"
  },
  "diagnostic-languageserver.formatters": {
    "shfmt": {
      "command": "shfmt",
      "args": ["-i", "2", "-bn", "-ci", "-sr"]
    }
  },
  "yaml.format.enable": true,
  "yaml.format.singleQuote": false,
  "yaml.schemas": {
    "http://json.schemastore.org/cloudbuild": "/cloudbuild*.yaml"
  }
}
