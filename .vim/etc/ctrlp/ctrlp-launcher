[minpac] update						:PackUpdate
[minpac] clean						:PackClean
[gina]   pull							:Gina pull
[gina]   push							:Gina push
[gina]   current log			:Gina log :%
[lsp]    def vsplit				:vertical LspDefinition
[lsp]    def split				:botright LspDefinition
[lsp]    deg tab					:tab LspDefinition
[toggle] number						:normal [Toggle]n
[toggle] relativenumber		:normal [Toggle]rn
[toggle] cursorline				:normal [Toggle]c
[toggle] wrap							:normal [Toggle]w
[toggle] paste						:normal [Toggle]p
[edit]   utf8							:edit ++enc=utf-8 %
[edit]   sjis							:edit ++enc=sjis %
[edit]   cp932						:edit ++enc=cp932 %
[etc]    open vimrc				:tabe $MYVIMRC
[etc]    startuptime			:StartupTime
